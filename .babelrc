{
  "comments": false,
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    [
      "@babel/plugin-transform-modules-commonjs",
      {
        "noInterop": false
      }
    ],
    "@babel/plugin-transform-runtime",
    "@babel/plugin-transform-async-to-generator"
  ],
  "presets": [
    [
      "@babel/env",
      {
        "modules": false
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [
        "@babel/env"
      ]
    }
  }
}
